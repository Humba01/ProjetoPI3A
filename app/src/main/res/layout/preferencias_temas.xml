<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="1dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FFF7DF">

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="#00FFEF63"
                android:foregroundTint="#B62C2C"
                android:padding="20px"
                android:tint="#000000"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="?attr/actionModeCloseDrawable"
                tools:layout_editor_absoluteX="3dp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="181dp"
                android:layout_height="42dp"
                android:fontFamily="@font/outfit_regular"
                android:gravity="center"
                android:padding="10sp"
                android:text="Temas"
                android:textAlignment="viewStart"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/imageButton3"
                tools:ignore="HardcodedText"
                tools:layout_editor_absoluteY="-2dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="20px"
            android:orientation="horizontal"
            android:padding="9sp">

            <CheckBox
                android:id="@+id/checkBox"
                android:layout_width="64dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:backgroundTint="#FFEB3B"
                android:checked="true"
                android:drawableTint="#000000"
                android:fontFamily="@font/notosans_condensed_regular"
                android:maxWidth="66dp"
                android:text="Claro"
                android:textAllCaps="false"
                android:textColorLink="#000000"
                app:buttonIconTint="#000000"
                app:buttonIconTintMode="src_in"
                app:checkedState="checked"
                app:useMaterialThemeColors="false"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="1"
                android:background="#fefefe" />

            <View
                android:id="@+id/view2"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="1"
                android:background="#FFF7DF" />

            <View
                android:id="@+id/view3"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="1"
                android:background="#FFEF63" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="20px"
            android:orientation="horizontal"
            android:padding="9sp">

            <CheckBox
                android:id="@+id/checkBox2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/notosans_condensed_regular"
                android:maxWidth="66dp"
                android:text="Escuro"
                app:buttonIconTint="#000000"
                app:buttonIconTintMode="src_in"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view4"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="1"
                android:background="#000000" />

            <View
                android:id="@+id/view5"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="1"
                android:background="#FDE49B" />

            <View
                android:id="@+id/view6"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="1"
                android:background="#F6DE00" />

        </LinearLayout>

        <TextView
            android:id="@+id/textView16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/notosans_regular"
            android:padding="4sp"
            android:text="Temas Experimentais"
            android:textSize="16sp"
            android:textStyle="bold|italic" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="20px"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:padding="9sp">

            <CheckBox
                android:id="@+id/checkBox3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/notosans_condensed_regular"
                android:maxWidth="66dp"
                android:text="Light Protanopia e Deuteranopia"
                app:buttonIconTint="#000000"
                app:buttonIconTintMode="src_in"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view7"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="1"
                android:background="#FFF591" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="20px"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:padding="9sp">

            <CheckBox
                android:id="@+id/checkBox4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/notosans_condensed_regular"
                android:maxWidth="66dp"
                android:text="Dark Protanopia e Deuteranopia"
                app:buttonIconTint="#000000"
                app:buttonIconTintMode="src_in"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view8"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="1"
                android:background="#FCE39A" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="20px"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:padding="9sp">

            <CheckBox
                android:id="@+id/checkBox5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/notosans_condensed_regular"
                android:maxWidth="66dp"
                android:text="Light Tritanopia"
                app:buttonIconTint="#000000"
                app:buttonIconTintMode="src_in"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view9"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="1"
                android:background="#FEF490" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="20px"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:padding="9sp">

            <CheckBox
                android:id="@+id/checkBox6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/notosans_condensed_regular"
                android:maxWidth="66dp"
                android:text="Dark Tritanopia"
                app:buttonIconTint="#000000"
                app:buttonIconTintMode="src_in"
                tools:ignore="HardcodedText" />

            <View
                android:id="@+id/view10"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_weight="1"
                android:background="#FBE299" />

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>